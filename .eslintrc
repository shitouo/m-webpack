{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel",
    "react",
    "promise"
  ],
  "env": {
    "browser" : true,
    "node" : true
  },
  "globals": {
    "$": false,
    "_hmt": false,
    "mraid": false,
    "share": false,
    "invite": false,
    "kingOfGlory": false,
    "setShareInfo": false
  },
  "rules": {
    "object-curly-spacing": [2, "always"],
    "no-multi-str": "off",
    "no-extend-native": "off",
    "no-undef": "off"
  }
}
